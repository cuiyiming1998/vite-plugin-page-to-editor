{
  "name": "vite-plugin-vue-page-to-editor",
  "type": "module",
  "version": "0.0.3",
  "packageManager": "pnpm@8.6.0",
  "description": "",
  "author": "cuiyiming <atlasyoungcym@163.com>",
  "license": "MIT",
  "homepage": "https://github.com/cuiyiming1998/vite-plugin-vue-page-to-editor#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cuiyiming1998/vite-plugin-vue-page-to-editor.git"
  },
  "bugs": "https://github.com/cuiyiming1998/vite-plugin-vue-page-to-editor/issues",
  "keywords": [
    "vite-plugin"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*"
      ]
    }
  },
  "files": [
    "dist",
    "*.d.ts"
  ],
  "scripts": {
    "build": "tsup && esno scripts/postbuild.ts",
    "dev": "tsup src/*.ts --watch src",
    "play": "npm -C playground run dev",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.4",
    "@antfu/ni": "^0.21.3",
    "@antfu/utils": "^0.7.4",
    "@types/node": "^18.16.16",
    "bumpp": "^9.1.0",
    "eslint": "^8.41.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.12",
    "lint-staged": "^13.2.2",
    "pnpm": "^8.6.0",
    "rimraf": "^5.0.1",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4",
    "vite": "^4.3.9"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
